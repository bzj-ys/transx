{
  "name": "transx",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rm -rf ./dist && vue-cli-service build --target lib --name transx --dest dist packages/index.js",
    "lint": "vue-cli-service lint",
    "start": "npm run serve"
  },
  "main": "dist/transx.umd.min.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "gsap": "^3.10.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.33",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue": "^3.2.31"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "transtion",
    "transx",
    "vue"
  ]
}
